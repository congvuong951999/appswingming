            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Techniques2"
            android:parentActivityName=".note" />
        <activity android:name=".FullScreen" />
        <activity
            android:name=".Techniques"
            android:parentActivityName=".note" />
        <activity
            android:name=".note"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SwimmingStyle"
